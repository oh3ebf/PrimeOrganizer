<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/application/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>openNoteBook</from-outcome>
            <to-view-id>/application/notebook.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>openBookmarks</from-outcome>
            <to-view-id>/application/bookmarks.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>openSettings</from-outcome>
            <to-view-id>/application/settings.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/application/notebook.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backToMain</from-outcome>
            <to-view-id>/application/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/application/bookmarks.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backToMain</from-outcome>
            <to-view-id>/application/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/application/settings.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backToMain</from-outcome>
            <to-view-id>/application/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
